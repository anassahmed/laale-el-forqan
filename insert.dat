INSERT INTO db_quran (othmani, imlai) values('وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبََا ۝١٩‌','وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَاباً');
