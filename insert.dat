INSERT INTO db_quran (othmani, imlai) values('كَلَّآ إِنَّ كِتَـٰبَ ٱلۡفُجَّارِ لَفِى سِجِّينِِ ۝٧‌','كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ');
