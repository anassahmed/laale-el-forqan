INSERT INTO db_quran (othmani, imlai) values('بَلۡ يُرِيدُ كُلُّ ٱمۡرِىِِٕ مِّنۡهُمۡ أَن يُؤۡتَىٰ صُحُفََا مُّنَشَّرَةََ ۝٥٢‌','بَلْ يُرِيدُ كُلُّ امْرِئٍ مِنْهُمْ أَنْ يُؤْتَى صُحُفاً مُنَشَّرَةً');
