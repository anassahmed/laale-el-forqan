INSERT INTO db_quran (othmani, imlai) values('أَفَبِهَـٰذَا ٱلۡحَدِيثِ أَنتُم مُّدۡهِنُونَ ۝٨١‌','أَفَبِهَذَا الْحَدِيثِ أَنْتُمْ مُدْهِنُونَ');
INSERT INTO db_quran (othmani, imlai) values('وَتَجۡعَلُونَ رِزۡقَكُمۡ أَنَّكُمۡ تُكَذِّبُونَ ۝٨٢‌','وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ');
INSERT INTO db_quran (othmani, imlai) values('فَلَوۡلَآ إِذَا بَلَغَتِ ٱلۡحُلۡقُومَ ۝٨٣‌','فَلَوْلَا إِذَا بَلَغَتِ الْحُلْقُومَ');
INSERT INTO db_quran (othmani, imlai) values('وَأَنتُمۡ حِينَٮِٕذِِ تَنظُرُونَ ۝٨٤‌','وَأَنْتُمْ حِينَئِذٍ تَنْظُرُونَ');
INSERT INTO db_quran (othmani, imlai) values('وَنَحۡنُ أَقۡرَبُ إِلَيۡهِ مِنكُمۡ وَلَـٰكِن لَّا تُبۡصِرُونَ ۝٨٥‌','وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْكُمْ وَلَكِنْ لَا تُبْصِرُونَ');
INSERT INTO db_quran (othmani, imlai) values('فَلَوۡلَآ إِن كُنتُمۡ غَيۡرَ مَدِينِينَ ۝٨٦‌','فَلَوْلَا إِنْ كُنْتُمْ غَيْرَ مَدِينِينَ');
