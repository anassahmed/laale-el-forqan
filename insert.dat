INSERT INTO db_quran (othmani, imlai) values('إِذۡ نَادَٮٰهُ رَبُّهُ ۥ بِٱلۡوَادِ ٱلۡمُقَدَّسِ طُوًى ۝١٦‌','إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوىً');
INSERT INTO db_quran (othmani, imlai) values('ٱذۡهَبۡ إِلَىٰ فِرۡعَوۡنَ إِنَّهُ ۥ طَغَىٰ ۝١٧‌','اذْهَبْ إِلَى فِرْعَوْنَ إِنَّهُ طَغَى');
INSERT INTO db_quran (othmani, imlai) values('فَقُلۡ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ ۝١٨‌','فَقُلْ هَلْ لَكَ إِلَى أَنْ تَزَكَّى');
INSERT INTO db_quran (othmani, imlai) values('وَأَهۡدِيَكَ إِلَىٰ رَبِّكَ فَتَخۡشَىٰ ۝١٩‌','وَأَهْدِيَكَ إِلَى رَبِّكَ فَتَخْشَى');
