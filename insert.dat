INSERT INTO db_quran (othmani, imlai) values('وَيۡلُُ يَوۡمَٮِٕذِِ لِّلۡمُكَذِّبِينَ ۝١٠‌','وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ');
