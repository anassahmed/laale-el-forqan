INSERT INTO db_quran (othmani, imlai) values('فَأَمَّآ إِن كَانَ مِنَ ٱلۡمُقَرَّبِينَ ۝٨٨‌','فَأَمَّا إِنْ كَانَ مِنَ الْمُقَرَّبِينَ');
