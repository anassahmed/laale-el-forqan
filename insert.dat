INSERT INTO db_quran (othmani, imlai) values('لَمَجۡمُوعُونَ إِلَىٰ مِيقَـٰتِ يَوۡمِِ مَّعۡلُومِِ ۝٥٠‌','لَمَجْمُوعُونَ إِلَى مِيقَاتِ يَوْمٍ مَعْلُومٍ');
INSERT INTO db_quran (othmani, imlai) values('ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ ۝٥١‌','ثُمَّ إِنَّكُمْ أَيُّهَا الضَّالُّونَ الْمُكَذِّبُونَ');
INSERT INTO db_quran (othmani, imlai) values('لَأَكِلُونَ مِن شَجَرِِ مِّن زَقُّومِِ ۝٥٢‌','لَآَكِلُونَ مِنْ شَجَرٍ مِنْ زَقُّومٍ');
INSERT INTO db_quran (othmani, imlai) values('فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ ۝٥٣‌','فَمَالِئُونَ مِنْهَا الْبُطُونَ');
INSERT INTO db_quran (othmani, imlai) values('فَشَـٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ ۝٥٤‌','فَشَارِبُونَ عَلَيْهِ مِنَ الْحَمِيمِ');
