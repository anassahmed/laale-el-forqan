INSERT INTO db_quran (othmani, imlai) values('وَٱلسَّـٰبِحَـٰتِ سَبۡحََا ۝٣‌','وَالسَّابِحَاتِ سَبْحاً');
INSERT INTO db_quran (othmani, imlai) values('فَٱلسَّـٰبِقَـٰتِ سَبۡقََا ۝٤‌','فَالسَّابِقَاتِ سَبْقاً');
