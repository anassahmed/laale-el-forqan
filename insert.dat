INSERT INTO db_quran (othmani, imlai) values('لَّا يَمَسُّهُ ۥۤ إِلَّا ٱلۡمُطَهَّرُونَ ۝٧٩‌','لَا يَمَسُّهُ إِلَّا الْمُطَهَّرُونَ');
